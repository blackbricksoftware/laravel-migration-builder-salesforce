    "type": "library",
    "require": {
        "php": "^8.1",
        "omniphx/forrest": "^2.11",
        "blackbricksoftware/laravel-migration-builder": "^10.0",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0"
    },
    "authors": [
        {
            "name": "David Hayes",
            "email": "david@blackbrick.software"
        }
    ],
    "autoload": {
        "psr-4": {
            "BlackBrickSoftware\\MigrationBuilderSalesforce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
